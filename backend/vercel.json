{
    "version": 2,
    "builds": [
        {
            "src": "*.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "methods": [
        "POST",
        "PUT",
        "DELETE",
        "GET"
    ],
    "headers": [
        {
            "Access-Control-Allow-Origin": "*"
        }
    ]
}